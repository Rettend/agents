{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "playground-solid",
  "account_id": "bee9950f4935b08fbc80f8e9ea881286",
  "main": "src/server.ts",
  "compatibility_date": "2025-02-19",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "ai": {
    "binding": "AI",
    "remote": true
  },
  "assets": {
    "binding": "ASSETS"
  },
  "durable_objects": {
    "bindings": [
      {
        "class_name": "Scheduler",
        "name": "Scheduler"
      },
      {
        "class_name": "Stateful",
        "name": "Stateful"
      },
      {
        "class_name": "Chat",
        "name": "Chat"
      },
      {
        "class_name": "Rpc",
        "name": "Rpc"
      }
    ]
  },
  "migrations": [
    {
      "new_sqlite_classes": ["Scheduler", "Stateful", "Chat", "Rpc"],
      "tag": "v1"
    }
  ]
}
